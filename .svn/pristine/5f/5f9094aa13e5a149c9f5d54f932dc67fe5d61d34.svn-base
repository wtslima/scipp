/* tabelas */
.vertical-align-top {
	vertical-align:top !important;
}
/* Título da tabela */
caption.lead {
	border-bottom: 2px solid #ddd;
  margin-bottom: 0;
}
.row-caption {
	margin-right: -8px;
  margin-left: -8px;
}
th .dropdown a,
th .dropdown a:focus {
  color: #333;
	text-decoration: none;
	white-space: nowrap;
}
th .dropdown a:hover,
th .dropdown a:active {
  color: #337ab7;
  text-decoration: none;
}
caption>h2, td>h2, th>h2, td>h3, th>h3 {
	margin: inherit;
	font-size: inherit;
	line-height: inherit;
	font-weight: inherit;
}
caption small {
	font-size: 60%;
	margin-top: 9px;
}
table .nav-pills li {
	margin-left:6px;
}
/* para usar com nav-pills */
.table>thead>tr>td.td-nav {
	padding: 3px 0;
	border-bottom: 2px solid #ddd;
}
.table>thead>tr>td.td-nav>.td-nav-group {
	padding: 3px 8px;
	display: inline-block;
}
.dropdown-table-head {
	float:left;
	margin-right:0.2em;
}
.label-status {
  font-size: 94%;
}
/* formularios */
legend a {
	text-decoration:none;
}
.nested-fieldset {
  border: 0 !important; 
  margin-bottom: 5px;
  font-weight: 700;
  font-size: 14px;
}
@media (max-width: 399px) {
	.table>thead>tr>td.td-nav>form.td-nav-group {
		display: block;
	}
}
@media (min-width: 400px) {
	.td-nav>.form-inline .form-group {
    display: inline-block;
    margin-bottom: 0;
    vertical-align: middle;
  }
  .td-nav>.form-inline .form-control {
    display: inline-block;
    width: auto;
    vertical-align: middle;
  }
	.td-nav>.form-inline .form-control-static {
    display: inline-block;
  }
  .td-nav>.form-inline .input-group {
    display: inline-table;
    vertical-align: middle;
  }
  .td-nav>.form-inline .input-group .input-group-addon,
  .td-nav>.form-inline .input-group .input-group-btn,
  .td-nav>.form-inline .input-group .form-control {
    width: auto;
  }
  .td-nav>.form-inline .input-group > .form-control {
    width: 100%;
  }
  .td-nav>.form-inline .control-label {
    margin-bottom: 0;
    vertical-align: middle;
  }
  .td-nav>.form-inline .radio,
  .td-nav>.form-inline .checkbox {
    display: inline-block;
    margin-top: 0;
    margin-bottom: 0;
    vertical-align: middle;
  }
  .td-nav>.form-inline .radio label,
  .td-nav>.form-inline .checkbox label {
    padding-left: 0;
  }
  .td-nav>.form-inline .radio input[type="radio"],
  .td-nav>.form-inline .checkbox input[type="checkbox"] {
    position: relative;
    margin-left: 0;
  }
  .td-nav>.form-inline .has-feedback .form-control-feedback {
    top: 0;
  }
}
