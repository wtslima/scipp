}
.ul-no-style {
	list-style-type: none; 
	padding-left:0; 
	margin-bottom:0;
}
.ol-no-style {
	padding-left:20px; 
	margin-bottom:0;
}
.list-group-item-style-none {
	list-style-type: none;
}
.dropdown-head {
	float:left;
	margin-right:0.5em;
}
.text-decoration-underline {
	text-decoration: underline;
}
/*h3 a.btn-block {
	text-decoration:none;
}*/
.panel .panel-heading .panel-title span.glyphicon {
	margin-right:10px;
}
dd {
	margin-bottom:20px;
}
.margin-top-20px {
	margin-top:20px;
}
.margin-top-10px {
	margin-top:10px;
}
.margin-right-20px {
	margin-right:20px;
}
.display-inline-block {
	display:inline-block;
}
@media (max-width: 400px) {
  .hidden-xxs {
    display: none !important;
  }
