.navbar {
  z-index: 1;
}
.navbar-nav {
  margin: 0 ;
}
.navbar-brand {
	padding: 12px 15px;
}
.dropdown-toggle.navbar-brand-system {
	padding-top: 12px;
  padding-bottom: 14px;
}
.dropdown-toggle.navbar-brand-system.text {
    padding-top: 15px;
    padding-bottom: 15px;
}
.dropdown.dropdown-navbar-header {
	display: inline-block;
	vertical-align: top;
	float:left;
}
.navbar-brand,
.navbar-brand-system,
.navbar-user {
	border-right: solid 1px #e9e7e9;
}
.table thead th{
    background-color:black;
    color: white;
}
@media (max-width: 767px) {
	.navbar-brand,
	.navbar-brand-system,
	.navbar-user {
		border-right: none;
	}
	.nav.navbar-nav {
		float:left;
	}
	.collapse.in {
		float: right;
	}
	.navbar-collapse {
		border: none;
	}
}
@media (max-width: 529px) {
	.nav.navbar-nav {
		border-bottom: solid 1px #e9e7e9;
		float:none;
	}
	.dropdown.dropdown-navbar-header {
		display: block;
		border-top: solid 1px #e9e7e9;
		float:none;
  }
	.dropdown.dropdown-navbar-header:first-child {
		clear: left;
	}
	.navbar-toggle {
		float: left;
		margin-left: 15px;
	}
	.collapse.in {
		float: none;
	}
}
.navbar-default .navbar-nav>.open>a.navbar-brand-system,
.navbar-default .navbar-nav>.open>a.navbar-brand-system:focus,
.navbar-default .navbar-nav>.open>a.navbar-user,
.navbar-default .navbar-nav>.open>a.navbar-user:focus {
	border-color: #e9e7e9;
}
.navbar-nav>li>a.navbar-user {
	padding-top: 15px;
  padding-bottom: 15px;
}
