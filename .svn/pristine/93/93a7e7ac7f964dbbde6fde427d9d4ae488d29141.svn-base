/* ESTILOS DA BARRA DO GOVERNO FEDERAL */

#wrapper-barra-brasil {
    max-width: 1170px !important
}

    #wrapper-barra-brasil > div.brasil-flag {
        min-width: 132px !important;
        height: 32px !important
    }

    #wrapper-barra-brasil > span.acesso-info {
        left: 152px !important
    }

/* ESTILOS DA BARRA SUPERIOR DO CERTIFIQ */

/*{ background:url(Images/fundo_barra.jpg) }*/
#barraSuperior {
    height: 35px;
    min-height: 35px;
    margin-bottom: 0px;
    background: url(data:image/jpg;base64,) repeat-x;
}

    #barraSuperior .navbar-brand {
        padding-top: 5px;
        border-right: solid 2px #dfdfdf
    }

    #barraSuperior ul.navbar-nav li a {
        padding-top: 6px
    }

    #barraSuperior > .container > * {
        float: left
    }

    #barraSuperior ul.navbar-nav {
        margin-top: 0px;
        margin-bottom: 0px
    }

#divLogoBarraSuperior {
    margin-top: 6px;
    margin-left: 15px;
    margin-right: 15px
}

#barraSuperior > .container, #barraSuperior ul.navbar-nav,
#barraSuperior ul.navbar-nav li, #barraSuperior .navbar-brand,
#barraSuperior ul.navbar-nav li a {
    height: 35px
}

/* ESTILO DO MENU SUPERIOR (MENU DO SISTEMA) */

#menuSuperior {
    background-color: #00356B;
    margin-bottom: 0px;
    border-radius: 0px;
    border-bottom-width: 0px;
    border-left-width: 0px;
    border-right-width: 0px
}

    #menuSuperior a {
        color: #fff;
        text-shadow: none;
        font-weight: bold
    }

        #menuSuperior a:active, #menuSuperior a:focus {
            outline: none
        }

    #menuSuperior button.navbar-toggle .icon-bar {
        background-color: #fff
    }

    #menuSuperior button.navbar-toggle:hover,
    #menuSuperior button.navbar-toggle:focus {
        background-color: #fff
    }

        #menuSuperior button.navbar-toggle:hover .icon-bar,
        #menuSuperior button.navbar-toggle:focus .icon-bar {
            background-color: #00356B
        }

    #menuSuperior a:hover, #menuSuperior a:focus {
        color: #77C1B2
    }

#navbarMenuSuperior li.dropdown.open a,
#navbarMenuSuperior li.dropdown.open a:focus,
#navbarMenuSuperior li.dropdown.open a:active {
    color: #fff;
    background-color: #77C1B2
}

#navbarMenuSuperior .dropdown.open {
    color: #fff;
    background-color: #77C1B2
}

    #navbarMenuSuperior .dropdown.open .dropdown-menu {
        background-color: #77C1B2
    }

        #navbarMenuSuperior .dropdown.open .dropdown-menu a:hover {
            color: #00356B;
            background-color: #77C1B2
        }

/* ESTILO DO MENU LATERAL (MENU PÚBLICO) */

.menuPublico ul {
    list-style: none;
    margin: 0px;
    padding: 0px
}
    /*{ background:url(Images/bg-lista.jpg) }*/
    .menuPublico ul li {
        padding: 3px 0px 8px 0px;
        width: 200px;
        float: left;
        background: url(data:image/jpg;base64,) no-repeat bottom
    }
        /*{ background:url(Images/ico-lista.png) }*/
        .menuPublico ul li a {
            color: #53727b;
            padding-left: 20px;
            float: left;
            font-size: 12px;
            line-height: 16px;
            background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAQAAAAHCAIAAACgB3uHAAAACXBIWXMAAAsSAAALEgHS3X78AAAAIGNIUk0AAHolAACAgwAA+f8AAIDpAAB1MAAA6mAAADqYAAAXb5JfxUYAAABISURBVHjaJMgxAYAgAADBByNYwGIKAaSMugsEMwrvwHjHfr1jDFWNAdLTVCCoE+1MYV66qxCF465CL3n51g3oJQMRaCUDwD8AsZQmlK2nAaQAAAAASUVORK5CYII=) no-repeat 8px 6px
        }

        .menuPublico ul li.no-background {
            background: none
        }

@media (max-width: 768px) {
    .menuPublico ul li {
        height: 40px
    }

        .menuPublico ul li:last-child {
            float: none;
            overflow: hidden
        }
}

/* ESTILOS DO BREADCRUMBS */

#breadcrumbs {
    margin-bottom: 10px
}

    #breadcrumbs ol {
        border-top: 8px solid #77C1B2;
        border-bottom: 1px solid #ccc;
        padding-top: 7px;
        padding-bottom: 4px
    }

        #breadcrumbs ol li {
            list-style: none;
            font-family: Verdana;
            font-size: 11px;
            display: inline-block;
            color: #537079;
            font-weight: bold;
        }

            #breadcrumbs ol li + li:before {
                content: " > ";
                color: #000;
                font-weight: normal
            }

            #breadcrumbs ol li a {
                color: #537079;
                text-decoration: none;
                font-weight: bold;
                margin-right: 4px;
            }

/* ESTILOS DO RODAPÉ */
html {
    position: relative;
    min-height: 100%
}

body {
    margin-bottom: 120px
}

footer {
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 120px;
    background-color: #EEEEEE;
    border-top: solid 1px #CECECE;
    border-bottom: solid 1px #CECECE;
    padding-top: 40px
}

    footer .text-muted {
        margin: 10px 0;
        font-size: 10px;
        text-transform: uppercase;
        color: #124675;
        font-weight: bold;
    }

    footer img {
        width: 137px;
        height: 30px
    }

/* TIPOGRAFIA */

.titulo, .tituloSistema {
    color: #497e79;
    font-family: Arial;
    font-weight: bold
}

.titulo {
    font-size: 18px
}

.tituloSistema {
    font-size: 30px;
    margin-top: 0px
}

h1, h2, h3, h4, h5, h6 {
    color: #00356B;
    font-weight: bold
}

.container-publico h1, .container-publico h2,
.container-publico h3, .container-publico h4,
.container-publico h5, .container-publico h6 {
    color: #53727b;
    font-family: Verdana;
    font-weight: normal
}

.container-publico hr {
    color: #53727b;
    background-color: #53727b;
    margin-top: 0px;
    height: 1px
}

.container-publico p {
    color: #53727b;
    text-align: justify
}

.padding-50 {
    padding: 50px
}

.field-validation-error {
    color: Red;
}

.versao {
    color: #666;
    font-family: Arial
}

textarea {
    resize: none
}

/* Datatables */

table tr th{
    background-color: black !important;
    color:white;
}

#historico table {
    table-layout: fixed
}

    #historico table td {
        word-wrap: break-word;
        overflow-wrap: break-word
    }

#tableConsultaUnidadeNegocio {
    width: 870px !important;
}

/* Estilos globais */

.form-inline .form-group {
    vertical-align: top;
}

div.container > h2 {
    margin-top: 1px
}

.acoesPesquisa {
    margin-top: 15px
}

.linkExportar, .linkExportar:hover,
.linkExportar:active, .linkExportar:link {
    color: #000;
    font-weight: bold;
    outline: none
}

/* Bootstrap Tabs */
div.tab-content {
    padding-top: 20px
}

ul.nav-tabs a:active, ul.nav-tabs a:focus {
    outline: none
}

/* ~/Home/ISOs */

.divTituloSistema {
    margin-top: 30px
}

.logoIsos {
    width: 220px;
    height: 71px
}

/* ~/Conta/Login */

.boxLogin {
    padding: 33px;
    background-color: #f6f6f6;
    border: solid 5px #f1f1f1
}

#EsqueciMinhaSenha {
    float: right;
    padding-top: 10px;
    color: #666;
    font-family: Arial;
    font-size: 13px
}

    #EsqueciMinhaSenha:hover, #EsqueciMinhaSenha:focus {
        text-decoration: underline;
        cursor: pointer
    }

    #EsqueciMinhaSenha:active {
        outline: none
    }

/* ~/Certificado/Incluir */

div.tab-pane span.texto {
    font-weight: bold
}

div.barraAcoes {
    float: right
}

    div.barraAcoes a {
        padding: 5px
    }

div.cliente, div.escopo,
div.cliente div.panel-body > div.panel {
    margin-bottom: 5px
}

div.tab-pane > div.row:first-child {
    margin-bottom: 10px
}

/* Relatórios */
.tabelaRelatorio td {
    text-align: center
}
